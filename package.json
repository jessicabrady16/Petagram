{
  "name": "famagram",
  "private": true,
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@rails/actioncable": "^6.0.2-1",
    "@rails/activestorage": "^6.0.2-1",
    "@rails/ujs": "^6.0.2-1",
    "bootstrap": "^5.3.7",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1",
    "sass": "^1.90.0",
    "turbolinks": "^5.2.0"
  },
  "version": "0.1.0",
  "devDependencies": {
    "esbuild": "^0.25.9"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
